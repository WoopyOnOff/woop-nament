								"exec": [
									"tests[\"Status code is 200\"] = responseCode.code === 200;",
									"",
									"tests[\"Response empty Ok!\"] = responseBody === \"null\";"
								]
							}
						}
					],
					"request": {
						"url": "http://localhost:3000/api/tournaments/{{tournamentId}}",
						"method": "GET",
						"header": [],
